<script setup lang="ts">
import { ref, onMounted, computed } from 'vue'
import { useRoute } from 'vue-router'
import { useI18n } from 'vue-i18n'
import { apiClient, type TestResult } from '../api/client'

const { t } = useI18n()
const route = useRoute()
const loading = ref(true)
const error = ref('')

const result = ref<TestResult | null>(null)

onMounted(async () => {
  const testId = parseInt(route.params.id as string)

  try {
    loading.value = true
    result.value = await apiClient.getTestResult(testId)
  } catch (err) {
    console.error('Failed to fetch test result:', err)
    error.value = err instanceof Error ? err.message : 'Failed to load test results'
  } finally {
    loading.value = false
  }
})

const getGradeConfig = computed(() => {
  if (!result.value) return { color: 'text-gray-600', bg: 'bg-gradient-to-br from-gray-500 to-gray-600', text: t('testResults.grade.notAnalyzed') }

  const configs: Record<string, { color: string; bg: string; text: string }> = {
    'A': { color: 'text-green-600 dark:text-green-400', bg: 'bg-gradient-to-br from-green-500 to-emerald-500', text: t('testResults.grade.excellent') },
    'B': { color: 'text-blue-600 dark:text-blue-400', bg: 'bg-gradient-to-br from-blue-500 to-cyan-500', text: t('testResults.grade.good') },
    'C': { color: 'text-yellow-600 dark:text-yellow-400', bg: 'bg-gradient-to-br from-yellow-500 to-orange-500', text: t('testResults.grade.fair') },
    'D': { color: 'text-orange-600 dark:text-orange-400', bg: 'bg-gradient-to-br from-orange-500 to-red-500', text: t('testResults.grade.poor') },
    'F': { color: 'text-red-600 dark:text-red-400', bg: 'bg-gradient-to-br from-red-500 to-pink-500', text: t('testResults.grade.failed') },
    'N/A': { color: 'text-gray-600 dark:text-gray-400', bg: 'bg-gradient-to-br from-gray-500 to-gray-600', text: t('testResults.grade.notAnalyzed') }
  }
  return configs[result.value.analysis.grade] || configs['N/A']
})

const getPerformanceLevel = (avg: number) => {
  if (avg < 200) return { text: t('testResults.performance.excellent'), color: 'text-green-600 dark:text-green-400', percent: 100 }
  if (avg < 500) return { text: t('testResults.performance.good'), color: 'text-blue-600 dark:text-blue-400', percent: 75 }
  if (avg < 1000) return { text: t('testResults.performance.fair'), color: 'text-yellow-600 dark:text-yellow-400', percent: 50 }
  return { text: t('testResults.performance.poor'), color: 'text-red-600 dark:text-red-400', percent: 25 }
}

const performanceLevel = computed(() => {
  if (!result.value) return { text: 'N/A', color: 'text-gray-600', percent: 0 }
  return getPerformanceLevel(result.value.metrics.httpReqDuration.avg)
})
</script>

